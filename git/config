[alias]
  a     = add
  s     = status
  d     = diff
  l     = log --oneline --graph --decorate --all
  co    = checkout
  br    = branch
  ca    = commit -v --amend
  ci    = commit -v
  df    = diff
  dc    = diff --cached
  ds    = diff --stat
  dt    = describe --tags --long
  sh    = show
  dtv   = describe --tags --long --match 'v*'
  pb    = pull --rebase
  ph    = push
  ups   = "!git push -u origin HEAD"
  st    = stash
  sp    = stash pop

[user]
	email = sol@hiqdev.com
	name = Andrii Vasyliev
[init]
  defaultBranch = master
[pull]
  ff = only
